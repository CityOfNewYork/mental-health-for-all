= extend('/slm/layouts/default')

- title = 'Comprehensive Psychiatric Emergency Services Program (CPEP) at Health + Hospitals | Services | Mental Health for All'
/ - description = '{{ SERVICE_DESCRIPTION }}'

- this.service = this.services.find(service => service.title == 'Comprehensive Psychiatric Emergency Services Program (CPEP) at Health + Hospitals')
- this.service.slug = 'comprehensive-psychiatric-emergency-services-program-cpep-at-health-hospitals';

= content('main')
  = partial('/slm/services/banner.slm')
  = partial('/slm/services/main.slm')

= content('scripts')
  javascript:
    window.addEventListener('load', () => {
      document.querySelectorAll('[data-js="lazy"]').forEach(i => {
        i.classList.remove('not-loaded');
        i.classList.add('loaded');
      });
    });