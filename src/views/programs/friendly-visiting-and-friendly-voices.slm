= extend('/slm/layouts/default')

- title = 'Friendly Visiting and Friendly VOICES | Services | Mental Health for All'
- this.service = this.services.find(service => service.title == 'Friendly Visiting and Friendly VOICES')
- this.service.slug = 'friendly-visiting-and-friendly-voices';

= content('main')
  = partial('/slm/services/banner.slm')
  = partial('/slm/services/main.slm')

= content('scripts')
  javascript:
    window.addEventListener('load', () => {
      document.querySelectorAll('[data-js="lazy"]').forEach(i => {
        i.classList.remove('not-loaded');
        i.classList.add('loaded');
      });
    });